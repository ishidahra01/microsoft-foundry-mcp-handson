{
    "version": "2.0",
    "extensions": {
        "http": {
            "routePrefix": ""
        }
    },
    "customHandler": {
        "description": {
            "defaultExecutablePath": "python",
            "workingDirectory": "",
            "arguments": ["function_app.py"]
        },
        "enableForwardingHttpRequest": true,
        "enableHttpProxyingRequest": true
    }
}