# Azure AI Foundry project endpoint
AZURE_AI_PROJECT_ENDPOINT=https://your-project.services.ai.azure.com/api/projects/your-project

# Existing Foundry Agent name (exact, case-sensitive)
AZURE_AI_PROJECT_AGENT_NAME=your-agent-name

# Optional: specific agent version
# AZURE_AI_PROJECT_AGENT_VERSION=1

# AG-UI server settings
AGUI_PATH=/ag-ui
AGUI_HOST=0.0.0.0
AGUI_PORT=8000

# CORS settings (comma-separated). In production, set explicit origins.
AGUI_CORS_ORIGINS=http://localhost:3000
